platform :ios, '10.0'
use_frameworks!

workspace 'TVTime.xcworkspace'

# You need to set target when you use CocoaPods 1.0.0 or later.
target 'TVTime' do
    project 'TVTime'
    pod 'AFDateHelper'
    pod 'Alamofire'
    pod 'AlamofireImage'
    pod 'AlamofireNetworkActivityIndicator'
    pod 'IBAnimatable'
    pod 'MGSwipeTableCell'
    pod 'SwiftyJSON'
end

target 'TVTimeSDK' do
    project 'TVTimeSDK/TVTimeSDK'
    pod 'AFDateHelper'
    pod 'Alamofire'
    pod 'SwiftyJSON'
end
